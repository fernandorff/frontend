{
  "name": "hub-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "local": "npx dotenv-cli -e .env.local -- next dev",
    "dev": "npx dotenv-cli -e .env.develop -- next dev",
    "build": "npx dotenv-cli -e .env.prod -- next build",
    "build:develop": "npx dotenv-cli -e .env.develop -- next build",
    "start": "next start",
    "start:develop": "npx dotenv-cli -e .env.develop -- next start",
    "lint": "next lint",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\""
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@ant-design/pro-components": "^2.6.43",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.2",
    "@reduxjs/toolkit": "^1.9.7",
    "antd": "^5.12.4",
    "axios": "^1.6.2",
    "cpf-cnpj-validator": "^1.0.3",
    "dayjs": "^1.11.10",
    "encrypt-storage": "^2.12.22",
    "framer-motion": "^10.16.16",
    "jwt-decode": "^3.1.2",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.47.0",
    "react-icons": "^4.12.0",
    "react-input-mask": "^2.0.4",
    "react-query": "^3.39.3",
    "react-redux": "^8.1.3",
    "redux-persist": "^6.0.0",
    "zod": "^3.22.4"
